  <body>
    <!-- navbar start -->
    <%- include("./partials/nav") %>
    <!-- navbar end -->

    <!-- carousel start -->
    <section aria-label="home page photos">
      <div class="carousel" data-carousel>
        <!-- carousel buttons start -->
        <div class="overlay"></div>
        <img
          class="carousel-button prev"
          src="images/live/icons/left-arrow.svg"
          alt="previous"
          data-carousel-button="prev"
        />
        <img
          class="carousel-button next"
          src="images/live/icons/right-arrow.svg"
          alt="next"
          data-carousel-button="next"
        />
        <!-- carousel buttons end -->
        <!-- slides start -->
        <ul data-slides>
          <li class="slide" data-active>
            <img
              class="slide-image image-1"
              src="images/live/scaled-down/home-page/carousel/FunFortheWholeFam.jpg"
              alt="family photo"
            />
          </li>
          <li class="slide">
            <img
              class="slide-image image-2"
              src="/images/live/scaled-down/home-page/carousel/MemoriesthatLast.jpg"
              alt="baby photo"
            />
          </li>
          <li class="slide">
            <img
              class="slide-image image-3"
              src="images/live/scaled-down/home-page/carousel/SmallBusiness.jpg"
              alt="small business photo"
            />
          </li>
          <li class="slide">
            <img
              class="slide-image image-4"
              src="images/live/scaled-down/home-page/carousel/HablamosEspanol.jpg"
              alt="hablamos español"
            />
          </li>
        </ul>
        <ul data-texts>
          <li class="slide-text" data-active-text>
            <h3 class="slide-title">Fun for the Whole Family</h3>
            <p class="slide-description">
              From mustaches to inflatable instruments, our props are sure to
              entertain adults and children
            </p>
          </li>
          <li class="slide-text slide-text-memories">
            <h3 class="slide-title">Memories that Last</h3>
            <p class="slide-description slide-description-memories">
              Our high resolution camera, rapid photo-strip printing, and
              cellular messaging capabilities make it easy to capture memories
              that will last a lifetime
            </p>
          </li>
          <li class="slide-text slide-text-business">
            <h3 class="slide-title">Small (but Mighty) Business</h3>
            <p class="slide-description slide-description-business">
              With clients like UCLA, LA County, and ABC Unified School
              District, you can count on us!
            </p>
          </li>
          <li class="slide-text slide-text-espanol">
            <h3 class="slide-title">Hablamos Español</h3>
            <p class="slide-description slide-description-espanol">
              Somos capaces de traducir nuestra pantalla, impresiones, y
              acesorios
            </p>
          </li>
        </ul>
        <!-- slides end -->
      </div>
    </section>
    <!-- carousel end -->

    <!-- social links start -->
    <section class="social-links">
      <ul>
        <li>
          <a href="#">
            <ion-icon name="logo-facebook"></ion-icon>
          </a>
        </li>
        <li>
          <a href="#">
            <ion-icon name="logo-instagram"></ion-icon>
          </a>
        </li>
        <li>
          <a href="#">
            <ion-icon name="logo-tiktok"></ion-icon>
          </a>
        </li>
      </ul>
    </section>
    <!-- social links end -->

    <!-- banner start -->
    <section class="banner">
      <div class="banner-text">
        <h2>
          Rent-A-Booth LA is a photo booth rental company based in Los Angeles.
        </h2>
        <p>We've made it our mission to capture your memories!</p>
      </div>
      <div class="banner-box box-1"></div>
      <div class="banner-box box-2"></div>
    </section>
